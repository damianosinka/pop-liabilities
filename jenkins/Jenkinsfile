pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'python3 main.py'
            }
        }
        stage('Test') {
            steps {
                sh 'Executing tests ...'
            }
        }
        stage('Deliver') {
            steps {
                sh 'Deploy to dev ...'
            }
        }
    }
}
